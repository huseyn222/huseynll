@model IEnumerable<_19final.Models.home>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/admin/Views/Shared/_Layout1.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.photourl)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.basliq)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.etrafli)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <img src="@Html.DisplayFor(modelItem => item.photourl)">

        </td>
        <td>
            @Html.DisplayFor(modelItem => item.basliq)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.etrafli)
        </td>
        <td>
            @Html.ActionLink("Details", "Details", new { id=item.ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.ID })
        </td>
    </tr>
}

</table>
